public class Alumno { 
9   private String nombreAlumno; 
10   private Curso []cursos; 
11 
 
12     public Alumno() { 
13     } 
14 
 
15     public Alumno(String nombreAlumno, Curso[] cursos) { 
16         this.nombreAlumno = nombreAlumno; 
17         this.cursos = cursos; 
18     } 
19 
 
20     public String getNombreAlumno() { 
21         return nombreAlumno; 
22     } 
23 
 
24     public void setNombreAlumno(String nombreAlumno) { 
25         this.nombreAlumno = nombreAlumno; 
26     } 
27 
 
28     public Curso[] getCursos() { 
29         return cursos; 
30     } 
31 
 
32     public void setCursos(Curso[] cursos) { 
33         this.cursos = cursos; 
34     } 
35 } 
